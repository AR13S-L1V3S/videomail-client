{
  "name": "videomail-client",
  "version": "2.0.0-alpha",
  "description": "A wicked npm package to record videos directly in the browser, wohooo!",
  "author": "Michael Heuberger <michael.heuberger@binarykitchen.com>",
  "contributors": [
    {
      "name": "Michael Heuberger",
      "email": "michael.heuberger@binarykitchen.com"
    }
  ],
  "homepage": "https://videomail.io",
  "repository": {
    "type": "git",
    "url": "https://github.com/binarykitchen/videomail-client.git"
  },
  "main": "dist/videomail-client.js",
  "scripts": {
    "test": "rollup -c env/test/rollup.config.js | tape-run",
    "patch": "./env/dev/release.sh --importance=patch",
    "minor": "./env/dev/release.sh --importance=minor",
    "major": "./env/dev/release.sh --importance=major",
    "std": "standard --verbose | snazzy",
    "prebuild": "yarn std --silent",
    "prebuild:dev": "yarn std --silent",
    "build": "rollup -c env/prod/rollup.config.js",
    "build:dev": "rollup -c env/dev/rollup.config.js -w",
    "examples": "browser-sync start --startPath 'examples/simple.html' --server --files 'dist/videomail-client.js,examples/**'",
    "start": "npm-run-all --parallel build:dev examples"
  },
  "standard": {
    "ignore": [
      "dist"
    ],
    "globals": [
      "ENV"
    ]
  },
  "engines": {
    "node": ">=8.1.0",
    "yarn": ">=0.24.5",
    "npm": ">=5.0.3"
  },
  "keywords": [
    "webcam",
    "video",
    "videomail",
    "encoder",
    "getusermedia",
    "audio",
    "recorder"
  ],
  "dependencies": {
    "animitter": "3.0.0",
    "audio-sample": "1.0.3",
    "caller": "1.0.1",
    "canvas-to-buffer": "1.0.9",
    "classlist.js": "1.1.20150312",
    "contains": "0.1.1",
    "core-js": "2.4.1",
    "create-error": "0.3.1",
    "defined": "1.0.0",
    "despot": "1.1.3",
    "document-visibility": "1.0.1",
    "element-closest": "2.0.2",
    "filesize": "3.5.10",
    "get-form-data": "1.2.5",
    "hidden": "1.1.1",
    "humanize-duration": "3.10.0",
    "hyperscript": "2.0.2",
    "insert-css": "2.0.0",
    "iphone-inline-video": "2.2.2",
    "is-power-of-two": "1.0.0",
    "keymirror": "0.1.1",
    "merge-recursive": "0.0.3",
    "number-is-integer": "1.0.1",
    "readystate": "0.3.0",
    "request-frame": "1.5.3",
    "superagent": "3.5.2",
    "ua-parser-js": "0.7.13",
    "websocket-stream": "5.0.0"
  },
  "devDependencies": {
    "body-parser": "1.17.2",
    "browser-sync": "2.18.12",
    "connect-send-json": "1.0.0",
    "npm-run-all": "4.0.2",
    "read-pkg": "2.0.0",
    "rollup": "0.43.0",
    "rollup-plugin-alias": "1.3.1",
    "rollup-plugin-babel": "2.7.1",
    "rollup-plugin-buble": "0.15.0",
    "rollup-plugin-cleanup": "1.0.1",
    "rollup-plugin-closure-compiler-js": "1.0.4",
    "rollup-plugin-commonjs": "8.0.2",
    "rollup-plugin-filesize": "1.4.2",
    "rollup-plugin-json": "2.3.0",
    "rollup-plugin-node-builtins": "2.1.2",
    "rollup-plugin-node-globals": "1.1.0",
    "rollup-plugin-node-resolve": "3.0.0",
    "rollup-plugin-progress": "0.4.0",
    "rollup-plugin-regenerator": "0.3.0",
    "rollup-plugin-replace": "1.1.1",
    "rollup-plugin-stylus-css-modules": "1.4.0",
    "rollup-watch": "4.0.0",
    "snazzy": "7.0.0",
    "standard": "10.0.2",
    "tape": "4.7.0",
    "tape-run": "3.0.0",
    "uppercamelcase": "3.0.0",
    "zora": "0.3.0"
  }
}
